{% extends 'base.html.twig' %}

{% block title %}Validation{% endblock %}

{% block body %}

    <div class="container">
        <div class="alert alert-dismissible alert-warning">
            <ul>
                {% for error in errors %}
                    <li class="mb-0">{{ error }}</li>
                {% endfor %}
            </ul>
        </div>
        <a href="{{ back }}" class="btn btn-primary">Back</a>
    </div>

{% endblock %}